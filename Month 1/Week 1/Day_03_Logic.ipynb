
credit = 720
monthly_income = 55000
has_existing_loan = False

if credit >= 750:
    if monthly_income > 50000:
        print("Status: Clear (Premium Account)")
    else:
        print("Status: Clear (Standard Account)")
        
elif credit >= 600 and credit <= 750:
    if not has_existing_loan:  
        print("Status: Conditional Approval (Requires Review)")
    else:
        print("Status: Rejected (High Debt Ratio)")
        
else:
    print("Status: Rejected (Low Credit )")
